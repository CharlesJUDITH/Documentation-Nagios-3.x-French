#: GettingStarted/quickstart-opensuse.xml:42
#: GettingStarted/quickstart-ubuntu.xml:92
msgid "Create Account Information"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:99
#: GettingStarted/quickstart-opensuse.xml:44
#: GettingStarted/quickstart-ubuntu.xml:94
msgid "Become the root user."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:101
#: GettingStarted/quickstart-opensuse.xml:46
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ $ </prompt><userinput>su -l</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:105
#: GettingStarted/quickstart-opensuse.xml:50
#: GettingStarted/quickstart-ubuntu.xml:100
msgid "Create a new nagios user account and give it a password."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:107
#: GettingStarted/quickstart-opensuse.xml:52
#: GettingStarted/quickstart-ubuntu.xml:102
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>/usr/sbin/useradd -m nagios</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>passwd nagios</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:112
#: GettingStarted/quickstart-opensuse.xml:64
#: GettingStarted/quickstart-ubuntu.xml:116
msgid ""
"Create a new nagcmd group for allowing external commands to be submitted "
"through the web interface. Add both the nagios user and the apache user to "
"the group."
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:124
#: GettingStarted/quickstart-opensuse.xml:76
#: GettingStarted/quickstart-ubuntu.xml:128
msgid "Download Nagios and the Plugins"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:126
#: GettingStarted/quickstart-opensuse.xml:78
#: GettingStarted/quickstart-ubuntu.xml:130
msgid "Create a directory for storing the downloads."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:128
#: GettingStarted/quickstart-opensuse.xml:80
#: GettingStarted/quickstart-ubuntu.xml:132
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>mkdir ~/downloads</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>cd ~/downloads</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:134
#: GettingStarted/quickstart-opensuse.xml:85
#: GettingStarted/quickstart-ubuntu.xml:137
msgid ""
"Download the source code tarballs of both Nagios and the Nagios plugins "
"(visit <ulink url=\"&url-nagios;download/\">&url-nagios;download/</ulink> "
"for links to the latest versions). At the time of writing, the latest "
"versions of Nagios and the Nagios plugins were 3.0.3 and 1.4.11, "
"respectively."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:141
#: GettingStarted/quickstart-opensuse.xml:91
#: GettingStarted/quickstart-ubuntu.xml:144
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>wget http://osdn.dl.sourceforge.net/sourceforge/nagios/nagios-3.0.2.tar.gz</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>wget http://osdn.dl.sourceforge.net/sourceforge/nagiosplug/nagios-plugins-1.4.11.tar.gz</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:148
#: GettingStarted/quickstart-opensuse.xml:98
#: GettingStarted/quickstart-ubuntu.xml:151
msgid "Compile and Install Nagios"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:150
#: GettingStarted/quickstart-opensuse.xml:100
#: GettingStarted/quickstart-ubuntu.xml:153 GettingStarted/upgrading.xml:46
msgid "Extract the Nagios source code tarball."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:152
#: GettingStarted/quickstart-opensuse.xml:102
#: GettingStarted/quickstart-ubuntu.xml:155
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>cd ~/downloads</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>tar xzf nagios-3.0.2.tar.gz</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>cd nagios-3.0.2</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:158
#: GettingStarted/quickstart-opensuse.xml:108
#: GettingStarted/quickstart-ubuntu.xml:161
msgid ""
"Run the Nagios configure script, passing the name of the group you created "
"earlier like so:"
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:161
#: GettingStarted/quickstart-opensuse.xml:110
#: GettingStarted/quickstart-ubuntu.xml:164
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>./configure --with-command-group=nagcmd</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:165
#: GettingStarted/quickstart-opensuse.xml:114
#: GettingStarted/quickstart-ubuntu.xml:168 GettingStarted/upgrading.xml:60
msgid "Compile the Nagios source code."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:167
#: GettingStarted/quickstart-opensuse.xml:115
#: GettingStarted/quickstart-ubuntu.xml:170
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>make all</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:171
#: GettingStarted/quickstart-opensuse.xml:118
#: GettingStarted/quickstart-ubuntu.xml:174
msgid ""
"Install binaries, init script, sample config files and set permissions on "
"the external command directory."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:174
#: GettingStarted/quickstart-opensuse.xml:121
#: GettingStarted/quickstart-ubuntu.xml:177
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>make install</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>make install-init</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>make install-config</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>make install-commandmode</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:186
#: GettingStarted/quickstart-opensuse.xml:133
#: GettingStarted/quickstart-ubuntu.xml:189
msgid "Customize Configuration"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:188
#: GettingStarted/quickstart-opensuse.xml:135
#: GettingStarted/quickstart-ubuntu.xml:191
msgid ""
"Sample <link linkend=\"configuringnagios-config\">configuration files</link> "
"have now been installed in the <filename class=\"directory\">/usr/local/"
"nagios/etc</filename> directory. These sample files should work fine for "
"getting started with Nagios. You'll need to make just one change before you "
"proceed..."
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:193
#: GettingStarted/quickstart-opensuse.xml:140
#: GettingStarted/quickstart-ubuntu.xml:196
msgid ""
"Edit the <filename>/usr/local/nagios/etc/objects/contacts.cfg</filename> "
"config file with your favorite editor and change the email address "
"associated with the nagiosadmin contact definition to the address you'd like "
"to use for receiving alerts."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:199
#: GettingStarted/quickstart-opensuse.xml:144
#: GettingStarted/quickstart-ubuntu.xml:202
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>vi /usr/local/nagios/etc/objects/contacts.cfg</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:205
#: GettingStarted/quickstart-opensuse.xml:150
#: GettingStarted/quickstart-ubuntu.xml:208
msgid "Configure the Web Interface"
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:210
#: GettingStarted/quickstart-opensuse.xml:155
#: GettingStarted/quickstart-ubuntu.xml:213
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>make install-webconf</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:222
#: GettingStarted/quickstart-opensuse.xml:166
#: GettingStarted/quickstart-ubuntu.xml:225
msgid "Restart Apache to make the new settings take effect."
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:230
#: GettingStarted/quickstart-opensuse.xml:174
#: GettingStarted/quickstart-ubuntu.xml:233
msgid "Compile and Install the Nagios Plugins"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:232
#: GettingStarted/quickstart-opensuse.xml:176
#: GettingStarted/quickstart-ubuntu.xml:235
msgid "Extract the Nagios plugins source code tarball."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:234
#: GettingStarted/quickstart-opensuse.xml:178
#: GettingStarted/quickstart-ubuntu.xml:237
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>cd ~/downloads</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>tar xzf nagios-plugins-1.4.11.tar.gz</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>cd nagios-plugins-1.4.11</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:240
#: GettingStarted/quickstart-opensuse.xml:184
#: GettingStarted/quickstart-ubuntu.xml:243
msgid "Compile and install the plugins."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:242
#: GettingStarted/quickstart-opensuse.xml:186
#: GettingStarted/quickstart-ubuntu.xml:245
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>./configure --with-nagios-user=nagios --with-nagios-group=nagios</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>make</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>make install</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:250
#: GettingStarted/quickstart-opensuse.xml:194
#: GettingStarted/quickstart-ubuntu.xml:253
msgid "Start Nagios"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:252
#: GettingStarted/quickstart-opensuse.xml:196
msgid ""
"Add Nagios to the list of system services and have it automatically start "
"when the system boots."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:255
#: GettingStarted/quickstart-opensuse.xml:199
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>chkconfig --add nagios</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>chkconfig nagios on</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:260
#: GettingStarted/quickstart-opensuse.xml:204
#: GettingStarted/quickstart-ubuntu.xml:262
msgid "Verify the sample Nagios configuration files."
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:266
#: GettingStarted/quickstart-opensuse.xml:210
#: GettingStarted/quickstart-ubuntu.xml:268
msgid "If there are no errors, start Nagios."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-fedora.xml:268
#: GettingStarted/quickstart-opensuse.xml:212
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>service nagios start</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:311
#: GettingStarted/quickstart-opensuse.xml:218
#: GettingStarted/quickstart-ubuntu.xml:276
msgid "Login to the Web Interface"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:313
#: GettingStarted/quickstart-opensuse.xml:220
#: GettingStarted/quickstart-ubuntu.xml:278
msgid ""
"You should now be able to access the Nagios web interface at the URL below. "
"You'll be prompted for the username (nagiosadmin) and password you specified "
"earlier."
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:317
#: GettingStarted/quickstart-opensuse.xml:224
#: GettingStarted/quickstart-ubuntu.xml:282
msgid "http://localhost/nagios/"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:319
#: GettingStarted/quickstart-opensuse.xml:226
#: GettingStarted/quickstart-ubuntu.xml:284
msgid ""
"Click on the <guimenuitem>Service Detail</guimenuitem> navbar link to see "
"details of what's being monitored on your local machine. It will take a few "
"minutes for Nagios to check all the services associated with your machine, "
"as the checks are spread out over time."
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-fedora.xml:326
#: GettingStarted/quickstart-opensuse.xml:233
#: GettingStarted/quickstart-ubuntu.xml:291
msgid "Other Modifications"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-fedora.xml:328
#: GettingStarted/quickstart-opensuse.xml:235
msgid ""
"Make sure your machine's firewall rules are configured to allow access to "
"the web server if you want to access the Nagios interface remotely."
msgstr ""

# type: Content of: <chapter><title>
#: GettingStarted/quickstart-opensuse.xml:10
msgid "&name-opensuse; Quickstart"
msgstr ""

# type: Content of: <chapter><abstract><para>
#: GettingStarted/quickstart-opensuse.xml:13
msgid ""
"This guide is intended to provide you with simple instructions on how to "
"install Nagios from source (code) on &name-opensuse; and have it monitoring "
"your local machine inside of 20 minutes. No advanced installation options "
"are discussed here - just the basics that will work for 95% of users who "
"want to get started."
msgstr ""

# type: Content of: <chapter><abstract><para>
#: GettingStarted/quickstart-opensuse.xml:19
msgid ""
"These instructions were written based on an &name-opensuse; 10.2 "
"installation."
msgstr ""

# type: Content of: <chapter><section><title>
#: GettingStarted/quickstart-opensuse.xml:24
#: GettingStarted/quickstart-ubuntu.xml:50
msgid "Required Packages"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:26
msgid ""
"Make sure you've installed the following packages on your &name-opensuse; "
"installation before continuing. You can use <command>yast</command> to "
"install packages under &name-opensuse;."
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:32
msgid "apache2"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:36
msgid "C/C++ development libraries"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:57
msgid "Create a new nagios group. Add the nagios user to the group."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-opensuse.xml:59
#: GettingStarted/quickstart-ubuntu.xml:111
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>/usr/sbin/groupadd nagios</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>/usr/sbin/usermod -G nagios nagios</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-opensuse.xml:68
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>/usr/sbin/groupadd nagcmd</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>/usr/sbin/usermod -G nagcmd nagios</userinput>\n"
"<prompt>linux:~ # </prompt><userinput>/usr/sbin/usermod -G nagcmd wwwrun</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:128
#: GettingStarted/quickstart-ubuntu.xml:184
msgid "Don't start Nagios yet - there's still more that needs to be done..."
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:152
msgid "Install the Nagios web config file in the Apache conf.d directory."
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:158
#: GettingStarted/quickstart-ubuntu.xml:217
msgid ""
"Create a nagiosadmin account for logging into the Nagios web interface. "
"Remember the password you assign to this account - you'll need it later."
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-opensuse.xml:162
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>htpasswd2 -c /usr/local/nagios/etc/htpasswd.users nagiosadmin</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-opensuse.xml:168
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>service apache2 restart</userinput>\n"
msgstr ""

# type: Content of: <chapter><section><screen>
#: GettingStarted/quickstart-opensuse.xml:206
#, no-wrap
msgid ""
"\n"
"<prompt>linux:~ # </prompt><userinput>/usr/local/nagios/bin/nagios -v /usr/local/nagios/etc/nagios.cfg</userinput>\n"
"\n"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:239
msgid "You can do this by:"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:243
msgid "Opening the control center"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:247
msgid ""
"Select <guimenuitem>Open Administrator Settings</guimenuitem> to open the "
"YaST administrator control center"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:252
msgid ""
"Select <guimenuitem>Firewall</guimenuitem> from the <guimenu>Security and "
"Users</guimenu> category"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:256
msgid ""
"Click the <guimenuitem>Allowed Services</guimenuitem> option in the Firewall "
"Configuration window"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:261
msgid ""
"Add <guimenuitem>HTTP Server</guimenuitem> to the allowed services list for "
"the <guimenuitem>External Zone</guimenuitem>"
msgstr ""

# type: Content of: <chapter><section><orderedlist><listitem><para>
#: GettingStarted/quickstart-opensuse.xml:266
msgid ""
"Click <guibutton>Next</guibutton> and <guibutton>Accept</guibutton> to "
"activate the new firewall settings"
msgstr ""

# type: Content of: <chapter><section><para>
#: GettingStarted/quickstart-opensuse.xml:271
msgid ""
"Configuring email notifications is outside the scope of this documentation. "
"Refer to your system documentation, search the web, or look to the <ulink "
"url=\"&url-nagios-community;\">NagiosCommunity.org wiki</ulink> for specific "
"instructions on configuring your &name-opensuse; system to send email "
"messages to external addresses."
msgstr ""
