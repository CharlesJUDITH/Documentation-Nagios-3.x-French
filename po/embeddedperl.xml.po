#: AdvancedTopics/embeddedperl.xml:149 Development/epnplugins.xml:11
msgid "Developing Plugins For Use With Embedded Perl"
msgstr ""

# type: Content of: <chapter><section><para>
#: AdvancedTopics/embeddedperl.xml:150
msgid ""
"Information on developing plugins for use with the embedded Perl interpreter "
"can be found <link linkend=\"development-epnplugins\">here</link>."
msgstr ""
