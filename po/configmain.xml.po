msgid "<anchor id=\"configuringnagios-configmain-broker_module\" xreflabel=\"Event Broker Modules\"/>Event Broker Modules"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4514
#, no-wrap
msgid "broker_module=&lt;modulepath&gt; [moduleargs]"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4520
#, no-wrap
msgid ""
"broker_module=/usr/local/nagios/bin/ndomod.o\n"
"              cfg_file=/usr/local/nagios/etc/ndomod.cfg"
msgstr ""

# type: Content of: <chapter><section><section><para>
#: ConfiguringNagios/configmain.xml:4527
msgid "This directive is used to specify an event broker module that should by loaded by Nagios at startup. Use multiple directives if you want to load more than one module. Arguments that should be passed to the module at startup are seperated from the module path by a space."
msgstr ""

# type: Content of: <chapter><section><section><warning><para>
#: ConfiguringNagios/configmain.xml:4533
msgid "Do NOT overwrite modules while they are being used by Nagios or Nagios will crash in a fiery display of SEGFAULT glory. This is a bug/limitation either in <function>dlopen()</function>, the kernel, and/or the filesystem.  And maybe Nagios..."
msgstr ""

# type: Content of: <chapter><section><section><para>
#: ConfiguringNagios/configmain.xml:4538
msgid "The correct/safe way of updating a module is by using one of these methods:"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4543
msgid "Shutdown Nagios, replace the module file, restart Nagios"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4548
msgid "While Nagios is running... delete the original module file, move the new module file into place, restart Nagios"
msgstr ""

# type: Content of: <chapter><section><section><title>
#: ConfiguringNagios/configmain.xml:4555
msgid "<anchor id=\"configuringnagios-configmain-debug_file\" xreflabel=\"Debug File\"/>Debug File"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4565
#, no-wrap
msgid "debug_file=&lt;file_name&gt;"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4571
#, no-wrap
msgid "debug_file=/usr/local/nagios/var/nagios.debug"
msgstr ""

# type: Content of: <chapter><section><section><para>
#: ConfiguringNagios/configmain.xml:4577
msgid "This option determines where Nagios should write debugging information. What (if any) information is written is determined by the <xref linkend=\"configuringnagios-configmain-debug_level\" /> and <xref linkend=\"configuringnagios-configmain-debug_verbosity\" /> options. You can have Nagios automaticaly rotate the debug file when it reaches a certain size by using the <xref linkend=\"configuringnagios-configmain-max_debug_file_size\" /> option."
msgstr ""

# type: Content of: <chapter><section><section><title>
#: ConfiguringNagios/configmain.xml:4586
msgid "<anchor id=\"configuringnagios-configmain-debug_level\" xreflabel=\"Debug Level\"/>Debug Level"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4596
#, no-wrap
msgid "debug_level=&lt;#&gt;"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4602
#, no-wrap
msgid "debug_level=24"
msgstr ""

# type: Content of: <chapter><section><section><para>
#: ConfiguringNagios/configmain.xml:4608
msgid "This option determines what type of information Nagios should write to the <xref linkend=\"configuringnagios-configmain-debug_file\" />. This value is a logical OR of the values below."
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4614
msgid "-1 = Log everything"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4618
msgid "0 = Log nothing (default)"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4622
msgid "1 = Function enter/exit information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4626
msgid "2 = Config information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4630
msgid "4 = Process information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4634
msgid "8 = Scheduled event information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4638
msgid "16 = Host/service check information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4642
msgid "32 = Notification information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4646
msgid "64 = Event broker information"
msgstr ""

# type: Content of: <chapter><section><section><title>
#: ConfiguringNagios/configmain.xml:4652
msgid "<anchor id=\"configuringnagios-configmain-debug_verbosity\" xreflabel=\"Debug Verbosity\"/>Debug Verbosity"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4662
#, no-wrap
msgid "debug_verbosity=&lt;#&gt;"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4668
#, no-wrap
msgid "debug_verbosity=1"
msgstr ""

# type: Content of: <chapter><section><section><para>
#: ConfiguringNagios/configmain.xml:4674
msgid "This option determines how much debugging information Nagios should write to the <xref linkend=\"configuringnagios-configmain-debug_file\" />."
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4679
msgid "0 = Basic information"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4683
msgid "1 = More detailed information (default)"
msgstr ""

# type: Content of: <chapter><section><section><itemizedlist><listitem><para>
#: ConfiguringNagios/configmain.xml:4687
msgid "2 = Highly detailed information"
msgstr ""

# type: Content of: <chapter><section><section><title>
#: ConfiguringNagios/configmain.xml:4693
msgid "<anchor id=\"configuringnagios-configmain-max_debug_file_size\" xreflabel=\"Maximum Debug File Size\"/>Maximum Debug File Size"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4703
#, no-wrap
msgid "max_debug_file_size=&lt;#&gt;"
msgstr ""

# type: Content of: <chapter><section><section><informaltable><tgroup><tbody><row><entry><para><programlisting>
#: ConfiguringNagios/configmain.xml:4709
#, no-wrap
msgid "max_debug_file_size=1000000"
msgstr ""

# type: Content of: <chapter><section><section><para>
#: ConfiguringNagios/configmain.xml:4715
msgid "This option determines the maximum size (in bytes) of the <link linkend=\"configuringnagios-configmain-debug_file\">debug file</link>. If the file grows larger than this size, it will be renamed with a .old extension. If a file already exists with a .old extension it will automatically be deleted. This helps ensure your disk space usage doesn't get out of control when debugging Nagios."
msgstr ""

